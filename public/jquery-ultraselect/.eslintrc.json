{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script",
        "ecmaFeatures": {
        }
    },
    "env": {
        "browser": true
    },
    "rules": {
        // possible errors
        "no-console": 0,
        "no-use-before-define": 2,

        // best practices
        "curly": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-eq-null": 2,
        //"no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-global-assign": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-param-reassign": 2,
        "no-proto": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-useless-escape": 2,
        "no-void": 2,
        "no-warning-comments": 1,
        "no-with": 2,
        "wrap-iife": 2,

        "strict": 2,

        // variables
        "no-catch-shadow": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "no-undef-init": 2,
        "no-undefined": 2,
        "no-use-before-define": 2,

        // stylistic issues
        "array-bracket-spacing": 2,
        "block-spacing": 2,
        "brace-style": 2,
        "camelcase": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "computed-property-spacing": 2,
        "consistent-this": 2,
        "eol-last": 2,
        "func-call-spacing": 2,
        "indent": 2,
        "key-spacing": 2,
        "keyword-spacing": 2,
        "line-comment-position": 2,
        "new-cap": 2,
        "new-parens": 2,
        "no-tabs": 2,
        "no-trailing-spaces": 2,
        "no-whitespace-before-property": 2,
        "object-curly-newline": 2,
        "object-curly-spacing": 2,
        "object-property-newline": 2,
        "one-var-declaration-per-line": 2,
        "quotes": 2,
        "semi-spacing": 2,
        "semi": 2,
        "space-before-blocks": 2,
        "space-in-parens": 2,
        "space-infix-ops": 2,
        "space-unary-ops": 2
    }
}